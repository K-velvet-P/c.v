<!-- highlighting words  -->
<script>
  window.onload = function () {
    const highlightElements = (className: string) => {
      const elements = document.querySelectorAll(
        `.${className}`
      ) as NodeListOf<HTMLSpanElement>;

      elements.forEach((span) => {
        span.addEventListener("mouseover", () => {
          elements.forEach((span) => {
            span.style.color = "white";
            span.style.background = "black";
          });
        });

        span.addEventListener("mouseout", () => {
          elements.forEach((span) => {
            span.style.color = "";
            span.style.background = "";
          });
        });
      });
    };

    highlightElements("node-span");
    highlightElements("react-span");
    highlightElements("JS-span");
    highlightElements("next-span");
    highlightElements("jest-span");
    highlightElements("playwright-span");
  };
</script>

